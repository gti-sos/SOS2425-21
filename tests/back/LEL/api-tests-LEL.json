		{
			"name": "identificador compuesto",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", () => {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Response is an object whose values are the matching records\", () => {\r",
							"    const body = pm.response.json();\r",
							"    pm.expect(body).to.be.an(\"object\");\r",
							"    \r",
							"    // Extraemos todos los registros (valores) en un array\r",
							"    const records = Object.values(body);\r",
							"    pm.expect(records).to.be.an(\"array\");\r",
							"    pm.expect(records.length, \"Debe devolver al menos un registro\").to.be.greaterThan(0);\r",
							"\r",
							"    // Obtenemos provincia/año de la URL\r",
							"    const path = pm.request.url.path; \r",
							"    const provinceParam = path[path.length-2].toLowerCase();\r",
							"    const yearParam = parseInt(path[path.length-1], 10);\r",
							"\r",
							"    // Comprobamos cada registro\r",
							"    records.forEach((rec, i) => {\r",
							"        pm.expect(rec).to.be.an(\"object\");\r",
							"        pm.expect(rec).to.have.property(\"province\").that.is.a(\"string\");\r",
							"        pm.expect(rec).to.have.property(\"year\").that.is.a(\"number\");\r",
							"        pm.expect(rec).to.have.property(\"transaction_total\").that.is.a(\"number\");\r",
							"        pm.expect(rec).to.have.property(\"transaction_protected_housing\").that.is.a(\"number\");\r",
							"        pm.expect(rec).to.have.property(\"transaction_new_housing\").that.is.a(\"number\");\r",
							"        pm.expect(rec).to.have.property(\"transaction_secondhand_housing\").that.is.a(\"number\");\r",
							"\r",
							"        // Opcional: verificar que coincidan con la petición\r",
							"        pm.expect(rec.province.toLowerCase(), `registro ${i}`).to.eql(provinceParam);\r",
							"        pm.expect(rec.year, `registro ${i}`).to.eql(yearParam);\r",
							"    });\r",
							"});\r",
							"\r",
							"\r",
							"\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
